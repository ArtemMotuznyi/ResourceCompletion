<idea-plugin>
    <id>com.github.artemmotuznyi.resourcecompletion</id>
    <name>ResourceCompletion</name>
    <vendor>artemmotuznyi</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <completion.contributor
                language="XML"
                implementationClass="com.github.artemmotuznyi.resourcecompletion.ResourceCompletionContributor"
        />
    </extensions>
</idea-plugin>
